{
  "singleQuote": false,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 100,
  "arrowParens": "always",
  "orderedImports": true,
  "jsxSingleQuote": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(\\.|\\.\\.)/(.(?!.(css|scss)))*$",
    "\\.(css|scss)$",
    "^react",
    "^next",
    "<THIRD_PARTY_MODULES>",
    "@assets/.*$",
    "@api/.*$",
    "@components/.*$",
    "@const/.*$",
    "@data/.*$",
    "@hooks/.*$",
    "@lib/.*$",
    "@pages/.*$",
    "@recoil/.*$",
    "@styles/.*$",
    "@types/.*$",
    "@utils/.*$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
